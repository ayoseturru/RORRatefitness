<h2 id="results_title" class="col col-xs-offset-1">Top 5 Centros Deportivos según los usuarios RateFit</h2>
<div id="centers_result" class="col-xs-12 col-sm-10 col-md-8 col-md-offset-2 col-lg-offset-1">
  <ul>
    <% @centers.each_with_index do |center, index| %>
        <h3 style="color: #ffa33f">Puesto Nº <%= index %> con <%= center.rate %></h3>
        <%= render center %>
    <% end %>
  </ul>
</div>

<script>
  $(document).ready(function () {
    $("#myNavbar ul li:nth-child(5)").addClass("active");
  });
</script>